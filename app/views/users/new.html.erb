<h1>Sign Up</h1>
<%= form_with(model: @user, url: users_path, local: true) do |f| %>

  <div>
    <%= f.label :first_name %>
    <%= f.text_field :firstNname %>
  <div>
  <div>
    <%= f.label :last_name %>
    <%= f.text_field :lastName %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </div>
  <div>
    <%= f.submit "Sign Up" %>
  </div>
<% end %>